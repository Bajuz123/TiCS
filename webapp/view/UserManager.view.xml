<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="TiCS.controller.UserManager">
	<App>
		<pages>
			<Page title="Title">
				<content>
				    <HBox width="100%" id="__hbox0">
				        <items>
				            <Button width="100px" id="__buttonAdd" icon="sap-icon://add" text="Add"/>
				            <Button text="Delete" width="100px" id="__buttonDelete" icon="sap-icon://delete"/>
				            <Button text="Edit" width="100px" id="__buttonEdit" icon="sap-icon://edit"/>
				        </items>
				    </HBox>
				    <Table noDataText="Drop column list items here and columns in the area above" id="__table0">
				        <items>
				            <ColumnListItem id="__item0">
				                <cells>
				                    <Text text="Row 1 Cell 1" id="__text0"/>
				                    <Text text="Row 1 Cell 2" id="__text1"/>
				                    <Text text="Row 1 Cell 3" id="__text2"/></cells>
				            </ColumnListItem>
				            <ColumnListItem id="__item1">
				                <cells>
				                    <Text text="Row 2 Cell 1" id="__text3"/>
				                    <Text text="Row 2 Cell 2" id="__text4"/>
				                    <Text text="Row 2 Cell 3" id="__text5"/></cells>
				            </ColumnListItem></items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="Header 1" id="__label0"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Header 2" id="__label1"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="Header 3" id="__label2"/>
				                </header>
				            </Column></columns></Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="TiCS.controller.Project" displayBlock="true">
	<App>
		<pages><Page title="{i18n>ProjectTitle}">
				<content><VBox width="100%" direction="Column" id="__vbox0">
						<items>
							<Table id="__tableProjects" growing="true" items="{tics>/PROJECT_SET}">
								<items>
									<ColumnListItem>
										<Text text="{tics>projektnummer}"/>
										<Text text="{tics>beschreibung}"/>
									</ColumnListItem>
								</items>
								<columns>
									<Column id="__columnProjID">
										<header>
											<Label text="Project Name" id="__label0"/>
										</header>
									</Column>
									<Column id="__columnProjName">
										<header>
											<Label text="Description" id="__label1"/>
										</header>
									</Column>
								</columns>
							</Table>
						</items>
					</VBox>
				</content>
			<headerContent>
			</headerContent>
			<footer>
			    <Bar class="sapContrast sapContrastPlus" id="__bar0">
			        <contentLeft>
			        </contentLeft>
			    <contentMiddle>
			    </contentMiddle>
			    <contentRight>
			        <Button xmlns="sap.m" text="Add" width="100px" id="__buttonAdd" icon="sap-icon://add" press="onAddClick"/>
			        <Button text="Delete" width="100px" id="__buttonDelete" icon="sap-icon://delete"/>
			        <Button text="Edit" width="100px" id="__buttonEdit" icon="sap-icon://edit"/>
			    </contentRight>
			    </Bar>
			</footer>
			</Page>
		</pages>
	</App>
</mvc:View>
<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="TiCS.controller.TicsDataDetail">
	<App>
		<pages><Page title="{i18n>TicsDetailTitle}">
			<content>
			    <sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="__form0">
			        <sap.ui.layout.form:formContainers>
			            <sap.ui.layout.form:FormContainer id="__containerTicsDetail">
			                <sap.ui.layout.form:formElements>
			                    <sap.ui.layout.form:FormElement id="__elementDate" label="{i18n>Date}">
			                        <sap.ui.layout.form:fields>
			                            <DatePicker width="35%" id="__picker0"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement id="__elementTimeFrom" label="{i18n>From}">
			                        <sap.ui.layout.form:fields>
			                            <TimePicker placeholder="HH:mm" placeholderSymbol="-" mask="h9:59:59" displayFormat="HH:mm" localeId="de-DE" id="__pickerTimeFrom" width="35%"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>To}" id="__element8">
			                        <sap.ui.layout.form:fields>
			                            <TimePicker placeholder="HH:mm" placeholderSymbol="-" mask="h9:59:59" displayFormat="HH:mm" localeId="de-DE" id="__pickerTimeTo" width="35%"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>Break}" id="__element1">
			                        <sap.ui.layout.form:fields>
			                            <Input width="35%" id="__input1" placeholder="0,5"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>ProjectNumber}" id="__elementProjNumber">
			                        <sap.ui.layout.form:fields>
			                            <ComboBox selectedItem="Element sap.ui.core.ListItem#__item0" value="List Item 1" selectedKey="item1" selectedItemId="__item0" id="__boxProjNumber" maxWidth="35%">
			                                <items>
			                                    <core:ListItem text="Project Number1" key="item1" id="__item12"/>
			                            		<core:ListItem text="Project Number2" key="item2" id="__item13"/>
			                            </items>
			                            </ComboBox>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>BillingKey}" id="__elementBK">
			                        <sap.ui.layout.form:fields>
			                            <ComboBox selectedItem="Element sap.ui.core.ListItem#__item3" value="List Item 1" selectedKey="item1" selectedItemId="__item3" id="__boxBK" maxWidth="35%">
			                                <items>
			                                     <core:ListItem text="Billing Key1" key="item1" id="__item9"/>
			                            		<core:ListItem text="Billing Key2" key="item2" id="__item10"/>
			                            	</items>
			                            </ComboBox>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>ProjectKey}" id="__elementPK">
			                        <sap.ui.layout.form:fields>
			                            <ComboBox selectedItem="Element sap.ui.core.ListItem#__item6" value="List Item 1" selectedKey="item1" selectedItemId="__item6" id="__boxPK" maxWidth="35%">
			                                <items>
			                                    <core:ListItem text="ProjectKey1" key="item1" id="__item6"/>
			                            		<core:ListItem text="ProjectKey2" key="item2" id="__item7"/>
			                            		</items>
			                            </ComboBox>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>Task}" id="__elementTask">
			                        <sap.ui.layout.form:fields>
			                            <ComboBox selectedItem="Element sap.ui.core.ListItem#__item11" value="List Item 1" selectedKey="item1" selectedItemId="__item11" id="__boxTask" maxWidth="35%">
			                                <items>
			                                     <core:ListItem text="Task1" key="item1" id="__item3"/>
			                            		<core:ListItem text="Task2" key="item2" id="__item4"/>
			                            	</items>
			                            </ComboBox>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>TaskDescription}" id="__elementTaskDescription">
			                        <sap.ui.layout.form:fields>
			                            <Input width="50%" id="__inputTaskDescription" editable="false"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                    <sap.ui.layout.form:FormElement label="{i18n>Comment}" id="__elementComment">
			                        <sap.ui.layout.form:fields>
			                            <Input width="100%" id="__inputComment"/>
			                        </sap.ui.layout.form:fields>
			                    </sap.ui.layout.form:FormElement>
			                </sap.ui.layout.form:formElements>
			            </sap.ui.layout.form:FormContainer>
			        </sap.ui.layout.form:formContainers>
			        <sap.ui.layout.form:layout>
			            <sap.ui.layout.form:ResponsiveGridLayout id="__layout0"/>
			        </sap.ui.layout.form:layout></sap.ui.layout.form:Form>
			</content>
			<footer>
			    <Bar class="sapContrast sapContrastPlus" id="__bar0">
			        <contentMiddle>
			        </contentMiddle>
			    <contentRight>
			        <Button width="100px" id="__buttonOK" icon="sap-icon://accept" />
			        <Button width="100px" id="__buttonCancel" icon="sap-icon://sys-cancel" press = "onCancelClick"/>
			    </contentRight>
			    </Bar>
			</footer>
			</Page>
		</pages>
	</App>

</mvc:View>
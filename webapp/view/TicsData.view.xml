<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="TiCS.controller.TicsData">
	<App>
		<pages><Page title="{i18n>TicsTitle}">
				<content>
				    <sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" width="100%" id="__layout1">
				    <sap.ui.layout:content>
				        <sap.ui.layout.form:Form editable="true" xmlns:sap.ui.layout.form="sap.ui.layout.form" id="__form0">
				            <sap.ui.layout.form:formContainers>
				                <sap.ui.layout.form:FormContainer title="Filter" id="__container0">
				                    <sap.ui.layout.form:formElements>
				                        <sap.ui.layout.form:FormElement label="{i18n>DateTo}" id="__labelDateTo">
				                            <sap.ui.layout.form:fields>
				                            	<DatePicker width="25%" id="__pickerTo"/>
				                            </sap.ui.layout.form:fields>
				                        </sap.ui.layout.form:FormElement>
				                         <sap.ui.layout.form:FormElement label="{i18n>DateFrom}" id="__labelDateFrom">
				                            <sap.ui.layout.form:fields>
				                            	<DatePicker width="25%" id="__pickerFrom"/>
				                            </sap.ui.layout.form:fields>
				                        </sap.ui.layout.form:FormElement>
				                    </sap.ui.layout.form:formElements>
				                </sap.ui.layout.form:FormContainer>
				            </sap.ui.layout.form:formContainers>
				            <sap.ui.layout.form:layout>
				                <sap.ui.layout.form:ResponsiveGridLayout id="__layout0"/>
				            </sap.ui.layout.form:layout></sap.ui.layout.form:Form>
				    </sap.ui.layout:content>
				    </sap.ui.layout:VerticalLayout>
				    <Table id="__tableTics" items="{tics>/TICS_SET}">
				        <items>
				            <ColumnListItem>
				                    <Text text="{tics>tag}"/>
				                	<Text text="{tics>vonzeit}"/>
				                	<Text text="{tics>erf_datum}"/>
				                	<Text text="{tics>erf_uhrzeit}"/>
				                   	<Text text="{tics>biszeit}"/>
				                	<Text text="{tics>pause}"/>
				                	<Text text="{tics>projektzeit}"/>
				                    <Text text="{tics>projektnummer}"/>
									<Text text="{tics>abrechnungsschl}"/>
									<Text text="{tics>projektschl}"/>
				                    <Text text="{tics>aufgabe}"/>
				                    <Text text="{tics>fefahrzeit}"/>
				                    <Text text="{tics>fakturierbarefah}"/>
				                    <Text text="{tics>id}"/>
				            </ColumnListItem></items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="Tag" id="__labelDay"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Vonzeit" id="__labelFrom"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="Erstellungs Datum" id="__labelCreateDate"/>
				                </header>
				            </Column>
				            <Column id="__column3">
				                <header>
				                    <Label text="Erstellung Zeit" id="__labelCreateTime"/>
				                </header>
				            </Column>
				             <Column id="__column4">
				                <header>
				                    <Label text="Biszeit" id="__labelTo"/>
				                </header>
				            </Column>
				              <Column id="__column5">
				                <header>
				                    <Label text="Pause" id="__labelBreak"/>
				                </header>
				            </Column>
				             <Column id="__column6">
				                <header>
				                    <Label text="Projektzeit" id="__labelProjectTime"/>
				                </header>
				            </Column><Column id="__column7">
				                <header>
				                    <Label text="Projektnummer" id="__labelProjectNumb"/>
				                </header>
				            </Column>
				             <Column id="__column8">
				                <header>
				                    <Label text="BK" id="__labelBK"/>
				                </header>
				            </Column>
				             <Column id="__column9">
				                <header>
				                    <Label text="PK" id="__labelPK"/>
				                </header>
				            </Column>
				             <Column id="__column10">
				                <header>
				                    <Label text="Aufgabe" id="__labelTask"/>
				                </header>
				            </Column>
				             <Column id="__column11">
				                <header>
				                    <Label text="Ferfahrzeit" id="__labelTravelTime"/>
				                </header>
				            </Column>
				             <Column id="__column12">
				                <header>
				                    <Label text="Fakturierbarfah" id="__labelFeTravelTime"/>
				                </header>
				            </Column>
				           </columns></Table>
				</content>
			<headerContent>
			</headerContent>
			<footer>
			    <Bar xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" id="__bar0_copy">
				            <contentMiddle>
				                </contentMiddle>
				            <contentRight>
				                <Button xmlns="sap.m" text="Filter" width="100px" id="__buttonAdd_copy2" icon="sap-icon://filter" press="onOKClick"/>
				                <Button xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" text="Add" width="100px" id="__buttonAdd_copy3" icon="sap-icon://add" press="onOKClick"/>
				                <Button xmlns="sap.m" text="Cancel" width="100px" id="__buttondelete" icon="sap-icon://delete" press="onCancelClick"/>
				                <Button xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" text="Edit" width="100px" id="__buttonEdit" icon="sap-icon://edit" press="onOKClick"/>
				            </contentRight>
				            </Bar>
			</footer>
			</Page>
		</pages>
	</App>
</mvc:View>
<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="TiCS.controller.TaskData">
	<App>
		<pages><Page title="{i18n>TaskTitle}">
				<content>
				    <sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" width="100%" id="__layout1">
				    <sap.ui.layout:content>
				        <Label text="{i18n>DateFrom}" width="100%" id="__label3"/>
				        <DatePicker width="25%" id="__pickerFrom"/>
				        <Label text="{i18n>DateTo}" width="100%" id="__label5"/>
				        <DatePicker width="25%" id="__pickerTo"/>
				        <Label text="{i18n>ProjectKey}" width="100%" id="__label4"/>
				        <Input width="100%" id="__inputParam2"/>
				        <Button text="{i18n>Filter}" width="100px" id="__buttonFilter" icon="sap-icon://filter"/>
				    </sap.ui.layout:content>
				    </sap.ui.layout:VerticalLayout>
				    <HBox width="100%" id="__hbox0">
				        <items>
				            <Button text="{i18n>Add}" width="100px" id="__buttonAdd" icon="sap-icon://add"/>
				            <Button text="{i18n>Delete}" width="100px" id="__buttonDelete" icon="sap-icon://delete"/>
				            <Button text="{i18n>Edit}" width="100px" id="__buttonEdit" icon="sap-icon://edit"/>
				        </items>
				    </HBox>
				    <Table noDataText="Drop column list items here and columns in the area above" id="__table0">
				        <items>
				            <ColumnListItem id="__item0">
				                <cells>
				                    <Text text="Row 1 Cell 1" id="__text5"/>
				                    <Text text="Row 1 Cell 2" id="__text6"/>
				                    <Text text="Row 1 Cell 3" id="__text7"/></cells>
				            </ColumnListItem>
				            <ColumnListItem id="__item1">
				                <cells>
				                    <Text text="Row 2 Cell 1" id="__text8"/>
				                    <Text text="Row 2 Cell 2" id="__text9"/>
				                    <Text text="Row 2 Cell 3" id="__text10"/></cells>
				            </ColumnListItem></items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="Header 1" id="__label0"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Header 2" id="__label1"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="Header 3" id="__label2"/>
				                </header>
				            </Column></columns></Table>
				</content>
			<headerContent>
			</headerContent>
			</Page>
		</pages>
	</App>
</mvc:View>
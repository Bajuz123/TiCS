<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="TiCS.controller.TaskData">
	<App>
		<pages><Page title="{i18n>TaskTitle}">
				<content>
				    <sap.ui.layout:VerticalLayout xmlns:sap.ui.layout="sap.ui.layout" width="100%" id="__layout1">
				    <sap.ui.layout:content>
				        <Label text="{i18n>DateFrom}" width="100%" id="__label3"/>
				        <DatePicker width="25%" id="__pickerFrom"/>
				        <Label text="{i18n>DateTo}" width="100%" id="__label5"/>
				        <DatePicker width="25%" id="__pickerTo"/>
				        <Label text="{i18n>ProjectKey}" width="100%" id="__label4"/>
				        <Input width="25%" id="__inputParam2"/>
				        <Button text="{i18n>Filter}" width="100px" id="__buttonFilter" icon="sap-icon://filter"/>
				    </sap.ui.layout:content>
				    </sap.ui.layout:VerticalLayout>
				    <HBox width="100%" id="__hbox0">
				        <items>
				            <Button text="{i18n>Add}" width="100px" id="__buttonAdd" icon="sap-icon://add"/>
				            <Button text="{i18n>Delete}" width="100px" id="__buttonDelete" icon="sap-icon://delete"/>
				            <Button text="{i18n>Edit}" width="100px" id="__buttonEdit" icon="sap-icon://edit"/>
				        </items>
				    </HBox>
				    <Table id="__tableTasks" items="{tics>/TASK_SET}">
				        <items>
				            <ColumnListItem>
				                    <Text text="{tics>tag}"/>
				                	<Text text="{tics>vonzeit}"/>
				                	<Text text="{tics>erf_datum}"/>
				                	<Text text="{tics>erf_uhrzeit}"/>
				                   	<Text text="{tics>biszeit}"/>
				                	<Text text="{tics>pause}"/>
				                	<Text text="{tics>projektzeit}"/>
				                    <Text text="{tics>projektnummer}"/>
									<Text text="{tics>abrechnungsschl}"/>
									<Text text="{tics>projektschl}"/>
				                    <Text text="{tics>aufgabe}"/>
				                    <Text text="{tics>fefahrzeit}"/>
				                    <Text text="{tics>fakturierbarefah}"/>
				                    <Text text="{tics>id}"/>
				            </ColumnListItem></items>
				        <columns>
				            <Column id="__column0">
				                <header>
				                    <Label text="Tag" id="__labelDay"/>
				                </header>
				            </Column>
				            <Column id="__column1">
				                <header>
				                    <Label text="Vonzeit" id="__labelFrom"/>
				                </header>
				            </Column>
				            <Column id="__column2">
				                <header>
				                    <Label text="Erstellungs Datum" id="__labelCreateDate"/>
				                </header>
				            </Column>
				            <Column id="__column3">
				                <header>
				                    <Label text="Erstellung Zeit" id="__labelCreateTime"/>
				                </header>
				            </Column>
				             <Column id="__column4">
				                <header>
				                    <Label text="Biszeit" id="__labelTo"/>
				                </header>
				            </Column>
				              <Column id="__column5">
				                <header>
				                    <Label text="Pause" id="__labelBreak"/>
				                </header>
				            </Column>
				             <Column id="__column6">
				                <header>
				                    <Label text="Projektzeit" id="__labelProjectTime"/>
				                </header>
				            </Column>
				             <Column id="__column7">
				                <header>
				                    <Label text="Projektnummer" id="__labelProjectNumb"/>
				                </header>
				            </Column>
				             <Column id="__column8">
				                <header>
				                    <Label text="BK" id="__labelBK"/>
				                </header>
				            </Column>
				             <Column id="__column9">
				                <header>
				                    <Label text="PK" id="__labelPK"/>
				                </header>
				            </Column>
				             <Column id="__column10">
				                <header>
				                    <Label text="Aufgabe" id="__labelTask"/>
				                </header>
				            </Column>
				             <Column id="__column11">
				                <header>
				                    <Label text="Ferfahrzeit" id="__labelTravelTime"/>
				                </header>
				            </Column>
				             <Column id="__column12">
				                <header>
				                    <Label text="Fakturierbarfah" id="__labelFeTravelTime"/>
				                </header>
				            </Column>
				           </columns></Table>
				</content>
			<headerContent>
			</headerContent>
			</Page>
		</pages>
	</App>
</mvc:View>
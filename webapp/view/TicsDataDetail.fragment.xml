<core:FragmentDefinition xmlns:core='sap.ui.core' xmlns='sap.m'>
	<Dialog title="Title TicsDetail">
		<beginButton>
			<Button xmlns="sap.m" width="50px" id="__buttonTicsOk" icon="sap-icon://accept" press="onOKClick"/>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" width="50px" id="__buttonTicsCancel" icon="sap-icon://sys-cancel" press="onCancelClick"/>
		</endButton>
		<content>
			<sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="__formTics">
				<sap.ui.layout.form:formContainers>
					<sap.ui.layout.form:FormContainer id="__containerTicsDetail">
						<sap.ui.layout.form:formElements>
							<sap.ui.layout.form:FormElement id="__elementTicsDate" label="{i18n>Date}">
								<sap.ui.layout.form:fields>
									<DatePicker width="35%" id="__pickerTicsDate" value="{SelectedTics>/tag}"/>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement id="__elementTicsTimeFrom" label="{i18n>From}">
								<sap.ui.layout.form:fields>
									<TimePicker placeholder="HH:mm" placeholderSymbol="-" mask="h9:59:59" displayFormat="HH:mm" localeId="de-DE" id="__pickerTimeFrom"
										width="35%" value="{SelectedTics>/vonzeit}"/>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>To}" id="__element8">
								<sap.ui.layout.form:fields>
									<TimePicker placeholder="HH:mm" placeholderSymbol="-" mask="h9:59:59" displayFormat="HH:mm" localeId="de-DE" id="__pickerTimeTo" width="35%" value="{SelectedTics>/biszeit}"/>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>Break}" id="__elementTicsBreak">
								<sap.ui.layout.form:fields>
									<Input width="35%" id="__inputTicsBreak" placeholder="0,5" value="{SelectedTics>/pause}" />
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>ProjectNumber}" id="__elementTicsProjNumber">
								<sap.ui.layout.form:fields>
									<ComboBox selectedItem="Element sap.ui.core.ListItem#__item0" value="List Item 1" selectedKey="item1" selectedItemId="__itemTicsProjNum"
										id="__boxProjNumber" maxWidth="35%">
										<items>
											<core:ListItem xmlns:core="sap.ui.core" text="Project Number1" key="item1" id="__itemTics12"/>
											<core:ListItem xmlns:core="sap.ui.core" text="Project Number2" key="item2" id="__itemTics13"/>
										</items>
									</ComboBox>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>BillingKey}" id="__elementBK">
								<sap.ui.layout.form:fields>
									<ComboBox selectedItem="Element sap.ui.core.ListItem#__item3" value="List Item 1" selectedKey="itemTicsBK" selectedItemId="__itemTicsBK" id="__boxBK"
										maxWidth="35%">
										<items>
											<core:ListItem xmlns:core="sap.ui.core" text="Billing Key1" key="item1" id="__itemTics9"/>
											<core:ListItem xmlns:core="sap.ui.core" text="Billing Key2" key="item2" id="__itemTics10"/>
										</items>
									</ComboBox>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>ProjectKey}" id="__elementPK">
								<sap.ui.layout.form:fields>
									<ComboBox selectedItem="Element sap.ui.core.ListItem#__item6" value="List Item 1" selectedKey="item1" selectedItemId="__itemTicsPK" id="__boxPK"
										maxWidth="35%">
										<items>
											<core:ListItem xmlns:core="sap.ui.core" text="ProjectKey1" key="item1" id="__itemTics6"/>
											<core:ListItem xmlns:core="sap.ui.core" text="ProjectKey2" key="item2" id="__itemTics7"/>
										</items>
									</ComboBox>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>Task}" id="__elementTask">
								<sap.ui.layout.form:fields>
									<ComboBox selectedItem="Element sap.ui.core.ListItem#__item11" value="List Item 1" selectedKey="itemTicsTask" selectedItemId="__itemTicsTask"
										id="__boxTask" maxWidth="35%">
										<items>
											<core:ListItem xmlns:core="sap.ui.core" text="Task1" key="item1" id="__itemTics3"/>
											<core:ListItem xmlns:core="sap.ui.core" text="Task2" key="item2" id="__itemTics4"/>
										</items>
									</ComboBox>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>TaskDescription}" id="__elementTaskDescription">
								<sap.ui.layout.form:fields>
									<Input width="50%" id="__inputTaskDescription" editable="false" />
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
							<sap.ui.layout.form:FormElement label="{i18n>Comment}" id="__elementComment">
								<sap.ui.layout.form:fields>
									<Input width="100%" id="__inputComment"/>
								</sap.ui.layout.form:fields>
							</sap.ui.layout.form:FormElement>
						</sap.ui.layout.form:formElements>
					</sap.ui.layout.form:FormContainer>
				</sap.ui.layout.form:formContainers>
				<sap.ui.layout.form:layout>
					<sap.ui.layout.form:ResponsiveGridLayout id="__layout0"/>
				</sap.ui.layout.form:layout>
			</sap.ui.layout.form:Form>
		</content>
	</Dialog>
</core:FragmentDefinition>
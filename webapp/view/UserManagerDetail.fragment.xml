<core:FragmentDefinition xmlns:core='sap.ui.core' xmlns='sap.m'>
	<Dialog title="Title UserDetail">
		<beginButton>
			<Button width="50px" id="__buttonUserOk" icon="sap-icon://accept" press="onOKClick"/>
		</beginButton>
		<endButton>
			<Button width="50px" id="__buttonUserCancel" icon="sap-icon://sys-cancel" press="onCancelClick"/>
		</endButton>
		<content>
			<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="__formUser">
				<sap.ui.layout.form:content>
					<Label text="{i18n>User_Personal_nr}" id="__labelUser_Personal_nr"/>
					<Input width="50%" id="__inputUser_Personal_nr"/>
					<Label text="{i18n>User_Role}" id="__labelRole"/>
					<Input width="50%" id="__inputRole"/>
					<Label text="{i18n>User_Calendar}" id="__labelUser_Calendar"/>
					<Input width="50%" id="__inputUser_Calendar"/>
					<Label text="{i18n>User_Username}" id="__labelUser_Username"/>
					<Input width="50%" id="__inputUser_Username"/>
				</sap.ui.layout.form:content>
			</sap.ui.layout.form:SimpleForm>
		</content>
		
	</Dialog>
</core:FragmentDefinition>
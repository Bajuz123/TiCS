<core:FragmentDefinition xmlns:core='sap.ui.core' xmlns='sap.m'>
	<Dialog title="Title UserDetail">
		<beginButton>
			<Button width="50px" id="__buttonUserOk" icon="sap-icon://accept" press="onOKClick"/>
		</beginButton>
		<endButton>
			<Button width="50px" id="__buttonUserCancel" icon="sap-icon://sys-cancel" press="onCancelClick"/>
		</endButton>
		<content>
			<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="__form0">
				<sap.ui.layout.form:content>
					<Label text="{i18n>User}" id="__labelUser"/>
					<Input width="50%" id="__inputUser"/>
					<Label text="{i18n>Permission}" id="__labelPermission"/>
					<Input width="50%" id="__inputPermission"/></sap.ui.layout.form:content>
			</sap.ui.layout.form:SimpleForm>
		</content>
		
	</Dialog>
</core:FragmentDefinition>
{
	"_version": "1.5.0",
	"sap.app": {
		"id": "TiCS",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"TICS": {
				"uri": "http://192.168.1.182/tics",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": []
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "TiCS.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"routing": {
			"config": {
				"viewType": "XML",
				"viewPath": "TiCS.view",
				"targetAggregation": "detailPages",
				"clearTarget": false
			},
			"routes": [{
				"pattern": "Logout",
				"name": "Logout",
				"view": "Logout",
				"targetAggregation": "pages"
			}, {
				"pattern": "Login",
				"name": "Login",
				"view": "Login",
				"targetAggregation": "pages"
			}, {
				"pattern": "",
				"name": "Menu",
				"view": "Menu",
				"targetAggregation": "masterPages",
				"targetControl": "splitapp",
				"subroutes": [{
					"pattern": "Project",
					"name": "Project",
					"view": "Project",
					"targetAggregation": "detailPages"
				}, {
					"pattern": "ProjectDetail",
					"name": "ProjectDetail",
					"view": "ProjectDetail",
					"targetAggregation": "detailPages"
				}, {
					"pattern": "TaskData",
					"name": "TaskData",
					"view": "TaskData",
					"targetAggregation": "detailPages"
				}, {
					"pattern": "Users",
					"name": "Users",
					"view": "UserManager",
					"targetAggregation": "detailPages"
				}, {
					"pattern": "TaskDataDetail",
					"name": "TaskDataDetail",
					"view": "TaskDataDetail",
					"targetAggregation": "detailPages"
				}]
			}]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "TiCS.i18n.i18n"
				}
			},
			"tics": {
				"dataSource": "TICS",
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}